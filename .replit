modules = ["nodejs-22", "python-3.11", "postgresql-16"]

[agent]
stack = "AGENT_STACK"
integrations = ["javascript_openai_ai_integrations:1.0.0", "agentstack-trigger-telegram:1.0.0", "replitmail:1.0.0", "javascript_anthropic_ai_integrations:1.0.0"]
expertMode = true
automationTriggerConnectorName = "legacy_telegram"

[nix]
channel = "stable-24_11"
packages = ["freetype", "imagemagick", "jq", "lcms2", "libimagequant", "libjpeg", "libtiff", "libwebp", "libxcrypt", "oneDNN", "openjpeg", "re2", "tcl", "tk", "zlib"]

[env]
MASTRA_HIDE_CLOUD_CTA = "true"

[deployment]
build = ["bash", "-c", "export NODE_OPTIONS='--max-old-space-size=4096' && npx tsx scripts/bumpVersion.ts patch && cd darkwave-web && npm run build && cd .. && rm -rf public/assets && cp -r darkwave-web/dist/* public/"]
run = ["npm", "run", "start"]
deploymentTarget = "autoscale"
publicDir = "darkwave-web/dist"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start inngest server"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start application"

[[workflows.workflow]]
name = "Start inngest server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "./scripts/inngest.sh"
waitForPort = 3000

[workflows.workflow.metadata]
outputType = "automations"

[[workflows.workflow]]
name = "Start application"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "./run-dev.sh"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

