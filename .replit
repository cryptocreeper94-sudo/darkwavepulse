modules = ["nodejs-22", "postgresql-16"]

[agent]
stack = "AGENT_STACK"
integrations = ["javascript_openai_ai_integrations:1.0.0", "agentstack-trigger-telegram:1.0.0", "replitmail:1.0.0"]
expertMode = true

[nix]
channel = "stable-24_11"
packages = ["jq", "imagemagick"]

[env]
MASTRA_HIDE_CLOUD_CTA = "true"

[deployment]
build = ["bash", "scripts/build.sh"]
run = ["sh", "-c", "cd .mastra/output && NODE_ENV=production node index.mjs"]
deploymentTarget = "cloudrun"

